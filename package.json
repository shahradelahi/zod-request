{
  "name": "zod-request",
  "version": "0.1.1",
  "description": "Validated and Type-safe HTTP requests using Zod",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "mocha \"**/*.test.ts\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint --fix \"src/**/*.ts\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm run test && pnpm run format:check && pnpm run lint && pnpm run build"
  },
  "packageManager": "pnpm@8.15.0",
  "dependencies": {
    "mustache": "^4.2.0",
    "undici-types": "^6.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shahradelahi/zod-request.git"
  },
  "keywords": [
    "zod",
    "request",
    "http",
    "fetch",
    "bun",
    "node",
    "type-safe"
  ],
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/shahradelahi/zod-request/issues"
  },
  "homepage": "https://github.com/shahradelahi/zod-request#readme"
}
