{
  "name": "zod-request",
  "version": "0.2.2",
  "description": "Validated and Type-safe HTTP requests using Zod",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "mocha \"**/*.test.ts\"",
    "type-check": "tsc --noEmit",
    "lint": "pnpm type-check & eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm build && pnpm test && pnpm format:check && pnpm lint"
  },
  "packageManager": "pnpm@8.15.3",
  "dependencies": {
    "mustache": "^4.2.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/chai": "^4.3.15",
    "@types/mocha": "^10.0.6",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.8.2",
    "typescript": "^5.4.5",
    "undici": "^6.15.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shahradelahi/zod-request.git"
  },
  "keywords": [
    "zod",
    "request",
    "http",
    "fetch",
    "browser",
    "node",
    "type-safe"
  ],
  "author": "Shahrad Elahi <shahrad@litehex.com> (https://github.com/shahradelahi)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shahradelahi/zod-request/issues"
  },
  "homepage": "https://github.com/shahradelahi/zod-request#readme"
}
